<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- -->
<hibernate-mapping>
  <class name="jpa.Admin" table="admin">
    <id column="ADMINID" name="adminid">
      <generator class="native"/>
    </id>
    <property name="adminlogin" type="string">
      <column length="255" name="ADMINLOGIN" not-null="true"/>
    </property>
    <property name="adminpassword" type="string">
      <column length="255" name="ADMINPASSWORD" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
