<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <application>
        <resource-bundle>
            <base-name>
                messages
            </base-name>
            <var>msg</var>
        </resource-bundle>
    </application>
    
    <managed-bean>
        <managed-bean-class>admin</managed-bean-class>    
        <managed-bean-name>web.forms.Log_Admin</managed-bean-name>    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-class>menuCandidat</managed-bean-class>    
        <managed-bean-name>web.forms.MenuCandidat</managed-bean-name>    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
     
    <managed-bean>
        <managed-bean-class>edit_candidat</managed-bean-class>    
        <managed-bean-name>web.forms.EditCandidat</managed-bean-name>    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
   
    <managed-bean>
        <managed-bean-class>menuTest</managed-bean-class>    
        <managed-bean-name>web.forms.MenuTest</managed-bean-name>    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-class>infoResa</managed-bean-class>    
        <managed-bean-name>web.forms.InfoReservation</managed-bean-name>    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-class>logCandidat</managed-bean-class>    
        <managed-bean-name>web.forms.Log_Candidat</managed-bean-name>    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-class>testInfo</managed-bean-class>    
        <managed-bean-name>web.forms.TestControler</managed-bean-name>    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-class>theCodeTester</managed-bean-class>    
        <managed-bean-name>web.forms.TheCodeTester</managed-bean-name>    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    
    <managed-bean>
        <managed-bean-class>recap</managed-bean-class>    
        <managed-bean-name>web.forms.Recapitulatif</managed-bean-name>    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    
    <managed-bean>
        <managed-bean-class>theEnd</managed-bean-class>    
        <managed-bean-name>web.forms.TheEnd</managed-bean-name>    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-class>create</managed-bean-class>    
        <managed-bean-name>web.forms.CreateTest</managed-bean-name>    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-class>oneForm</managed-bean-class>    
        <managed-bean-name>web.forms.Form1</managed-bean-name>    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-class>secondForm</managed-bean-class>    
        <managed-bean-name>web.forms.Form2</managed-bean-name>    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>


